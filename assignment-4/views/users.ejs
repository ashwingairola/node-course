<!DOCTYPE html>
<html lang="en">

	<head>
		<%- include('includes/head.ejs') %>
	</head>

	<body class="bg-app">
		<%- include('includes/navbar.ejs') %>
		<main class="main">
			<h1>Users</h1>

			<% if(users.length) { %>
			<ul>
				<% users.forEach(user => { %>
				<li><%= user.username %></li>
				<% }) %>
			</ul>
			<% } else { %>
			<h3 class="text-warn">There are no users.</h3>
			<% } %>
		</main>
	</body>

</html>