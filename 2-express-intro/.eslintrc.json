{
	"extends": ["plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"env": { "node": true },
	"parser": "babel-eslint",
	"parserOptions": { "sourceType": "module", "ecmaVersion": 2019 },
	"rules": {
		"prettier/prettier": [
			"warn",
			{
				"endOfLine": "auto"
			}
		],
		"no-unused-vars": "warn",
		"no-undef": "error"
	}
}
