{
	"extends": ["plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"env": { "node": true },
	"parser": "babel-eslint",
	"parserOptions": { "sourceType": "module", "ecmaVersion": 2019 },
	"rules": {
		"prettier/prettier": [
			"warn",
			{
				"endOfLine": "auto"
			}
		],
		"no-unused-vars": "warn",
		"no-undef": "error",
		"no-const-assign": "error",
		"no-var": "error",
		"no-dupe-args": "error",
		"no-else-return": "warn",
		"no-useless-return": "warn",
		"no-shadow": "warn",
		"no-empty": "warn",
		"no-invalid-this": "error",
		"no-undefined": "error"
	}
}
